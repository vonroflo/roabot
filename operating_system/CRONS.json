[
  {
    "name": "heartbeat",
    "schedule": "*/30 * * * *",
    "type": "agent",
    "job": "Read the file at operating_system/HEARTBEAT.md and complete the tasks described there.",
    "enabled": true
  },
  {
    "name": "ping",
    "schedule": "*/1 * * * *",
    "type": "command",
    "command": "echo \"pong!\"",
    "enabled": true
  },
  {
    "name": "daily-weather",
    "schedule": "0 5 * * *",
    "type": "command",
    "command": "node event_handler/tools/weather.js",
    "enabled": true
  }
]
